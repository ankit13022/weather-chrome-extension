{
    "manifest_version": 3,
    "name": "My weather Extension",
    "version": "1.0.0",
    "description": "A simple Chrome extension for weather",
    "action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icons/cloudy-16.png",
            "48": "icons/cloudy-48.png",
            "128": "icons/cloudy-128.png"
        }
    },
    "permissions": [
        "activeTab",
        "geolocation"
    ],
    "icons": {
        "16": "icons/cloudy-16.png",
        "48": "icons/cloudy-48.png",
        "128": "icons/cloudy-128.png"
    }
}